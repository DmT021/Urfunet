        Title="Ynot DFS Emulator" Height="768" Width="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="79*"/>
        </Grid.RowDefinitions>
        <TabControl x:Name="mytc" ItemsSource="{Binding Clients}" Grid.Row="2">

        </TabControl>
        <Button Content="Добавить узел" HorizontalAlignment="Left" Margin="4,1,0,0" VerticalAlignment="Top" Width="92" Click="Button_Click"/>

    </Grid>
</Window>
