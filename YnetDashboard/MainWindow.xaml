<Window x:Class="YnetDashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="672.333" Closed="Window_Closed_1">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="29*"/>
            <RowDefinition Height="291*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="349*"/>
            <ColumnDefinition Width="168*"/>
            <ColumnDefinition Width="168*"/>
        </Grid.ColumnDefinitions>
        <ListBox ItemsSource="{Binding Log}" Grid.Row="1"/>
        <ListBox ItemsSource="{Binding Clients}" SelectedItem="{Binding SelectedClient}" Grid.Row="1" Grid.Column="1"/>
        <Button Content="Начать" HorizontalAlignment="Left" Margin="10,0,0,7" Width="75" Click="Button_Click_1"/>
        <Button Content="тест связи" HorizontalAlignment="Left" Margin="103,0,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <Button Content="Button" Grid.Column="2" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_3" />
        <ListBox Grid.Column="2" ItemsSource="{Binding files}" Grid.Row="1"/>

    </Grid>
</Window>
